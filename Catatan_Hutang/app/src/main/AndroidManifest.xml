<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.catatan_hutang">

    <application
        android:allowBackup="true"
        android:icon="@drawable/wallet"
        android:label="@string/app_name"
        android:roundIcon="@drawable/wallet"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DetailCatatan"></activity>
        <activity android:name=".Tambah" />
        <activity
            android:name=".content_main"
            android:label="@string/title_activity_content_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>